<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>
		<Console name="CONSOLE">
			<PatternLayout pattern="%d{HH:mm:ss.SSS}|%-5level|%c{10}: %msg%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Logger name="nl.naturalis" level="DEBUG" additivity="false">
			<AppenderRef ref="CONSOLE" />
		</Logger>
		<Root level="ERROR">
			<AppenderRef ref="CONSOLE" />
		</Root>
	</Loggers>
</Configuration>
